     IF a(4, i) < 0 THEN a(4, i) = 0
'enhancing reaction
     a(5, i) = olddecaydiffE + re * a * a(9, i) / g * (e * e / f + be)
     a(6, i) = olddecaydiffF + rf * e * e / g
     a(7, i) = olddecaydiffG + rg * e

