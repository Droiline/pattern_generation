    bq = sb * a(0, 2) * b * b: a(2, i) = olddecaydiffB + (bq + f * a(1, i)) / e
    aq = sA * a(0, 1) * a * a: a(1, i) = olddecaydiffA + aq / e
    a(5, i) = olddecaydiffE + aq + bq + cq + dq
    a(6, i) = ba * a(8, i)

